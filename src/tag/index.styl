@import '../components/left-menu/_leftMenu.styl'
@import '../components/title/_title.styl'
@import '../global.styl'
@import '../components/video-list/_videoList.styl'
@import '../components/pagination/pagination.styl'

*
    padding: 0
    margin: 0
    box-sizing: border-box

body
    background: #f8f8f8
    font-size: 10px

ul li
    list-style: none

a
    text-decoration: none
    color: titleColor
    &:hover
        color: aHoverColor

.page-container
    padding-left: 20rem

.label
    background: mainColor
    color: #fff
    font-size: .5rem
    padding: 3px 6px
    border-radius: 3px